{
  "name": "my-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "concurrently --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check --loglevel=warn src",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext=js,jsx,ts,tsx,astro --max-warnings=0 src",
    "format": "eslint --ext=js,jsx,ts,tsx,astro --fix src && prettier --write src"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.5.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-triple": "^1.2.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.32.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.13.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "astro": "^5.6.1"
  }
}
